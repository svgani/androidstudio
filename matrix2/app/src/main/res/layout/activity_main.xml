<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TableLayout
        android:id="@+id/tableLayout3"
        android:layout_width="wrap_content"
        android:layout_height="238dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.23000002">

        <TableRow
            android:id="@+id/tableRow1a"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="15dp">

            <TextView
                android:id="@+id/a1"
                android:layout_width="100dp"
                android:layout_height="40dp"
                android:gravity="center|start"
                android:lineSpacingExtra="2dp"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="20sp"
                android:translationX="5dp" />

            <TextView
                android:id="@+id/a2"
                android:layout_width="match_parent"
                android:layout_height="40dp"

                android:gravity="center"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/a3"
                android:layout_width="100dp"
                android:layout_height="40dp"

                android:gravity="center|end"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="20sp" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow2a"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="15dp">

            <TextView
                android:id="@+id/b1"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:gravity="center|start"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="20sp"
                android:translationX="5dp" />

            <TextView
                android:id="@+id/b2"
                android:layout_width="80dp"
                android:layout_height="40dp"

                android:gravity="center"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/b3"
                android:layout_width="wrap_content"
                android:layout_height="40dp"

                android:gravity="center|end"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="20sp" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow3a"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:padding="15dp">

            <TextView
                android:id="@+id/c1"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:gravity="center|start"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="20sp"
                android:translationX="5dp" />

            <TextView
                android:id="@+id/c2"
                android:layout_width="wrap_content"
                android:layout_height="40dp"

                android:gravity="center"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/c3"
                android:layout_width="wrap_content"
                android:layout_height="40dp"

                android:gravity="center|end"
                android:textColor="@android:color/holo_green_dark"
                android:textSize="20sp" />

        </TableRow>

    </TableLayout>

    <TableLayout
        android:id="@+id/tableLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.924"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3">

        <TableRow
            android:id="@+id/tableRowa1"
            android:layout_width="178dp"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/editTexta1"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

            <EditText
                android:id="@+id/editTexta2"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

            <EditText
                android:id="@+id/editTexta3"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRowa2"
            android:layout_width="177dp"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/editTexta4"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

            <EditText
                android:id="@+id/editTexta5"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

            <EditText
                android:id="@+id/editTexta6"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRowa3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/editTexta7"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

            <EditText
                android:id="@+id/editTexta8"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

            <EditText
                android:id="@+id/editTexta9"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

        </TableRow>

    </TableLayout>

    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/tableLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.108"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout2">

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="178dp"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/editText1"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

            <EditText
                android:id="@+id/editText2"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

            <EditText
                android:id="@+id/editText3"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="177dp"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/editText4"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

            <EditText
                android:id="@+id/editText5"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

            <EditText
                android:id="@+id/editText6"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/editText7"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

            <EditText
                android:id="@+id/editText8"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

            <EditText
                android:id="@+id/editText9"
                android:layout_width="57dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="numberSigned|numberDecimal"
                android:text="0" />

        </TableRow>

    </TableLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="60dp"
        android:text="Matrix A"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/tableLayout2"
        app:layout_constraintHorizontal_bias="0.417"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tableLayout2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:text="Matrix B"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/tableLayout1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toEndOf="@+id/tableLayout1" />

    <Button
        android:id="@+id/button"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="81dp"
        android:layout_height="38dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:onClick="clear"
        android:text="Clear"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button2"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:onClick="find"
        android:text="Multiply"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout1"
        app:layout_constraintVertical_bias="0.362" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="101dp"
        android:background="#ff8e51"
        android:gravity="center"
        android:text="Matrix"
        android:textColor="#ffffff"
        android:textSize="24sp"
        app:fontFamily="monospace"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:text="Result"
        android:textColor="#000000"
        android:textSize="24sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/back"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="69dp"
        android:layout_height="39dp"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:onClick="back"
        android:text="Back"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout3"
        app:layout_constraintVertical_bias="0.24" />

    <android.support.constraint.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

